<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>randomHD playlist</name>
    <match>all</match>
    <rule field="title" operator="doesnotcontain">*</rule>
    <rule field="videoresolution" operator="greaterthan">710</rule>
    <limit>25</limit>
    <order direction="ascending">random</order>
</smartplaylist>
