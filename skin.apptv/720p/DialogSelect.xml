<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>1000</defaultcontrol>
	<coordinates>
		<left>0</left>
		<top>0</top>
	</coordinates>
	<controls>
		<include>fadebackground</include>
		<control type="image"><!-- Dialog background image -->
			<left>120</left>
			<top>60</top>
			<width>1040</width>
			<height>532</height>
			<texture border="9">button-menu-background.png</texture>
		</control>
		<control type="grouplist">
			<left>140</left>
			<top>72</top>
			<width>996</width>
			<height>37</height>
			<align>center</align>
			<aligny>center</aligny>
			<orientation>horizontal</orientation>
			<control type="label" id="1"><!-- Dialog heading -->
				<width max="900">auto</width>
				<height>37</height>
				<wrapmultiline>true</wrapmultiline>
				<aligny>top</aligny>
				<font>apptv25</font>
			</control>
			<control type="label"><!-- Number of files -->
				<width max="96">auto</width>
				<height>37</height>
				<label>$VAR[SelectDialogNumObjects]</label>
				<aligny>top</aligny>
				<font>apptv25</font>
			</control>
		</control>
		<control type="image">
			<left>140</left>
			<top>106</top>
			<width>996</width>
			<height>4</height>
			<texture>separator.png</texture>
		</control>
		<control type="group" id="1000">
			<control type="list" id="3"><!-- Type 3 Select -->
				<left>160</left>
				<top>124</top>
				<width>700</width>
				<height>448</height>
				<onleft condition="String.IsEqual(Skin.String(show-scrollbar),$LOCALIZE[31877])">noop</onleft>
				<onleft condition="!String.IsEqual(Skin.String(show-scrollbar),$LOCALIZE[31877])">23</onleft>
				<onright condition="!Control.IsVisible(5)">7</onright>
				<onright condition="Control.IsVisible(5)">5</onright>
				<pagecontrol>23</pagecontrol>
				<itemlayout width="700" height="64">
					<control type="image">
						<left>0</left>
						<top>0</top>
						<width>700</width>
						<height>64</height>
						<texture border="6">button-default.png</texture>
					</control>
					<control type="group">
						<visible>!String.IsEmpty(ListItem.Label2)</visible>
						<control type="label">
							<left>10</left>
							<top>8</top>
							<width>680</width>
							<height>24</height>
							<font>apptv22</font>
							<selectedcolor>button-text</selectedcolor>
							<label>$INFO[ListItem.Label]</label>
							<include condition="Skin.HasSetting(text-autoscroll)">scrolltext</include>
						</control>
						<control type="label">
							<left>10</left>
							<top>36</top>
							<width>680</width>
							<height>24</height>
							<font>apptv20</font>
							<selectedcolor>button-text</selectedcolor>
							<label>[I]$INFO[ListItem.Label2, - ] [/I]</label>
							<include condition="Skin.HasSetting(text-autoscroll)">scrolltext</include>
						</control>
					</control>
					<control type="label">
						<left>10</left>
						<top>20</top>
						<width>680</width>
						<height>24</height>
						<font>apptv23</font>
						<selectedcolor>button-text</selectedcolor>
						<label>$INFO[ListItem.Label]</label>
						<include condition="Skin.HasSetting(text-autoscroll)">scrolltext</include>
						<visible>String.IsEmpty(ListItem.Label2)</visible>
					</control>
				</itemlayout>
				<focusedlayout width="700" height="64">
					<control type="image">
						<left>0</left>
						<top>0</top>
						<width>700</width>
						<height>64</height>
						<texture border="6">button-default-focus.png</texture>
						<animation condition="!Control.HasFocus(3)" effect="fade" start="100" end="60">Conditional</animation>
					</control>
					<control type="group">
						<visible>!String.IsEmpty(ListItem.Label2)</visible>
						<control type="label">
							<left>10</left>
							<top>8</top>
							<width>680</width>
							<height>24</height>
							<font>apptv22</font>
							<selectedcolor>button-text</selectedcolor>
							<label>$INFO[ListItem.Label]</label>
							<include condition="Skin.HasSetting(text-autoscroll)">scrolltext</include>
						</control>
						<control type="label">
							<left>10</left>
							<top>36</top>
							<width>680</width>
							<height>24</height>
							<font>apptv20</font>
							<selectedcolor>button-text</selectedcolor>
							<label>[I]$INFO[ListItem.Label2, - ] [/I]</label>
							<include condition="Skin.HasSetting(text-autoscroll)">scrolltext</include>
						</control>
					</control>
					<control type="label">
						<left>10</left>
						<top>20</top>
						<width>680</width>
						<height>24</height>
						<font>apptv23</font>
						<selectedcolor>button-text</selectedcolor>
						<label>$INFO[ListItem.Label]</label>
						<include condition="Skin.HasSetting(text-autoscroll)">scrolltext</include>
						<visible>String.IsEmpty(ListItem.Label2)</visible>
					</control>
				</focusedlayout>
			</control>
			<control type="list" id="6"><!-- Type 6 Select -->
				<left>145</left>
				<top>124</top>
				<width>700</width>
				<height>448</height>
				<onright condition="String.IsEqual(Skin.String(show-scrollbar),$LOCALIZE[31877])">noop</onright>
				<onright condition="!String.IsEqual(Skin.String(show-scrollbar),$LOCALIZE[31877])">26</onright>
				<onup>6</onup>
				<ondown>6</ondown>
				<pagecontrol>26</pagecontrol>
				<itemlayout width="700" height="64">
					<control type="image">
						<left>0</left>
						<top>2</top>
						<height>52</height>
						<width>52</width>
						<texture fallback="DefaultFile.png">$INFO[ListItem.Icon]</texture>
						<aspectratio>keep</aspectratio>
					</control>
					<control type="image">
						<left>54</left>
						<top>0</top>
						<width>646</width>
						<height>56</height>
						<texture border="6">button-default.png</texture>
					</control>
					<control type="label">
						<left>70</left>
						<top>-4</top>
						<width>614</width>
						<height>56</height>
						<font>apptv22</font>
						<selectedcolor>button-text</selectedcolor>
						<label>$INFO[ListItem.Label] [I]$INFO[ListItem.Label2, - ] [/I]</label>
						<include condition="Skin.HasSetting(text-autoscroll)">scrolltext</include>
					</control>
				</itemlayout>
				<focusedlayout width="700" height="64">
					<control type="image">
						<left>0</left>
						<top>2</top>
						<height>52</height>
						<width>52</width>
						<texture fallback="DefaultFile.png">$INFO[ListItem.Icon]</texture>
						<aspectratio>keep</aspectratio>
					</control>
					<control type="image">
						<left>54</left>
						<top>0</top>
						<width>646</width>
						<height>56</height>
						<texture border="6">button-default-focus.png</texture>
						<animation condition="!Control.HasFocus(6)" effect="fade" start="100" end="60">Conditional</animation>
					</control>
					<control type="label">
						<left>70</left>
						<top>-4</top>
						<width>614</width>
						<height>56</height>
						<font>apptv22</font>
						<selectedcolor>button-text</selectedcolor>
						<label>$INFO[ListItem.Label] [I]$INFO[ListItem.Label2, - ] [/I]</label>
						<include condition="Skin.HasSetting(text-autoscroll)">scrolltext</include>
					</control>
				</focusedlayout>
			</control>
		</control>
		<control type="scrollbar" id="23">
			<left>136</left>
			<top>124</top>
			<width>16</width>
			<height>448</height>
			<onright>3</onright>
			<animation condition="!Control.HasFocus(23) + !Container.Scrolling + !String.IsEqual(Skin.String(show-scrollbar),$LOCALIZE[31875])" effect="fade" start="100" end="0" time="200" reversible="true">Conditional</animation>
			<visible>Control.IsVisible(3) + !String.IsEqual(Skin.String(show-scrollbar),$LOCALIZE[31877])</visible>
		</control>
		<control type="group">
			<visible>Control.IsVisible(6)</visible>
			<control type="scrollbar" id="26">
				<left>856</left>
				<top>124</top>
				<width>16</width>
				<height>448</height>
				<onleft>6</onleft>
				<onright>7</onright>
				<animation condition="!Control.HasFocus(26) + !Container.Scrolling + !String.IsEqual(Skin.String(show-scrollbar),$LOCALIZE[31875])" effect="fade" start="100" end="0" time="200" reversible="true">Conditional</animation>
				<visible>$EXP[isEnabledScrollbar]</visible>
			</control>
			<control type="image">
				<left>884</left>
				<top>246</top>
				<height>256</height>
				<width>256</width>
				<texture fallback="DefaultFile.png">$INFO[Container(6).ListItem.Icon]</texture>
				<aspectratio aligny="top">keep</aspectratio>
				<animation effect="fade" time="300">VisibleChange</animation>
			</control>
		</control>
		<control type="button" id="5"><!-- Manual -->
			<left>884</left>
			<top>124</top>
			<width>256</width>
			<height>56</height>
			<font>apptv23</font>
			<label>$LOCALIZE[186]</label>
			<align>center</align>
			<ondown>7</ondown>
			<onleft condition="Control.IsVisible(3)">3</onleft>
			<onleft condition="Control.IsVisible(6)">6</onleft>
		</control>
		<control type="button" id="7"><!-- Cancel -->
			<left>884</left>
			<top>180</top>
			<width>256</width>
			<height>56</height>
			<font>apptv23</font>
			<align>center</align>
			<onup>5</onup>
			<onleft condition="Control.IsVisible(3)">3</onleft>
			<onleft condition="Control.IsVisible(6)">6</onleft>
		</control>
	</controls>
</window>