<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">5</defaultcontrol>
	<coordinates>
		<left>0</left>
		<top>0</top>
	</coordinates>
	<controls>
		<include>fadebackground</include>
		<control type="image"><!-- Dialog background image -->
			<left>214</left>
			<top>60</top>
			<width>848</width>
			<height>600</height>
			<texture border="9">button-menu-background.png</texture>
		</control>
		<control type="label" id="2"><!-- Dialog heading -->
			<left>224</left>
			<top>70</top>
			<width>828</width>
			<height>25</height>
			<align>center</align>
			<font>apptv25</font>
		</control>
		<control type="image">
			<left>224</left>
			<top>100</top>
			<width>828</width>
			<height>4</height>
			<texture>separator.png</texture>
		</control>
		<control type="grouplist" id="5"><!-- Default Grouplist -->
			<left>244</left>
			<top>126</top>
			<width>788</width>
			<height>450</height>
			<pagecontrol>60</pagecontrol>
			<onleft>noop</onleft>
			<onright>60</onright>
			<onup>noop</onup>
			<ondown>9000</ondown>
		</control>
		<control type="button" id="7"><!-- Default Button -->
			<height>50</height>
			<font>apptv23</font>
		</control>
		<control type="radiobutton" id="8"><!-- Default Radiobutton -->
			<height>50</height>
			<font>apptv23</font>
		</control>
		<control type="spincontrolex" id="9"><!-- Default Spincontrolex -->
			<height>50</height>
			<font>apptv23</font>
		</control>
		<control type="image" id="11"><!-- Default Seperator -->
			<height>4</height>
			<texture>separator.png</texture>
		</control>
		<control type="edit" id="12"><!-- Default Edit -->
			<height>50</height>
			<font>apptv23</font>
		</control>
		<control type="sliderex" id="13"><!-- Default Sliderex -->
			<height>50</height>
			<font>apptv23</font>
		</control>
		<control type="label" id="14"><!-- Default Label -->
			<height>50</height>
			<font>apptv23</font>
			<textoffsetx>18</textoffsetx>
		</control>
		<control type="scrollbar" id="60">
			<left>1036</left>
			<top>124</top>
			<width>16</width>
			<height>450</height>
			<onright>5</onright>
			<animation condition="!Control.HasFocus(60) + !Container.Scrolling + !String.IsEqual(Skin.String(show-scrollbar),$LOCALIZE[31875])" effect="fade" start="100" end="0" time="200" reversible="true">Conditional</animation>
			<visible>$EXP[isEnabledScrollbar]</visible>
		</control>
		<control type="group"><!-- Markers for control area items -->
			<control type="image">
				<left>630</left>
				<top>106</top>
				<height>16</height>
				<width>16</width>
				<texture>scroll-up.png</texture>
				<visible>Container(5).HasPrevious</visible>
			</control>
			<control type="image">
				<left>630</left>
				<top>578</top>
				<height>16</height>
				<width>16</width>
				<texture>scroll-down.png</texture>
				<visible>Container(5).HasNext</visible>
			</control>
		</control>
		<control type="grouplist" id="9000">
			<left>248</left>
			<top>600</top>
			<width>780</width>
			<height>50</height>
			<align>center</align>
			<orientation>horizontal</orientation>
			<itemgap>0</itemgap>
			<onup>5</onup>
			<ondown>5</ondown>
			<control type="button" id="28"><!-- OK -->
				<width>260</width>
				<height>50</height>
				<align>center</align>
				<font>apptv23</font>
				<onleft>noop</onleft>
				<onright>29</onright>
				<onup>5</onup>
			</control>
			<control type="button" id="29"><!-- Cancel -->
				<width>260</width>
				<height>50</height>
				<align>center</align>
				<font>apptv23</font>
				<onleft>28</onleft>
				<onright>30</onright>
				<onup>5</onup>
			</control>
			<control type="button" id="30"><!-- Clear/Settings -->
				<width>260</width>
				<height>50</height>
				<align>center</align>
				<font>apptv23</font>
				<onleft>29</onleft>
				<onright>noop</onright>
				<onup>5</onup>
			</control>
		</control>
	</controls>
</window>