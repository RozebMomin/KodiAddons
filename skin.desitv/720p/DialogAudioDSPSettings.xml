<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog">
	<defaultcontrol>5</defaultcontrol>
	<coordinates>
		<left>170</left>
		<top>40</top>
	</coordinates>
	<onload>Dialog.Close(osdaudiosettings)</onload>
	<controls>
		<control type="image"><!-- Dialog background Non Fullscreen Video screens -->
			<left>0</left>
			<top>0</top>
			<width>926</width>
			<height>642</height>
			<texture border="8">button-menu-background.png</texture>
			<visible>!Window.IsActive(fullscreenvideo)</visible>
		</control>
		<control type="image"><!-- Dialog background Fullscreen Video screen -->
			<left>0</left>
			<top>0</top>
			<width>926</width>
			<height>642</height>
			<texture colordiffuse="UnFocusedPosters" border="8">button-menu-background.png</texture>
			<visible>Window.IsActive(fullscreenvideo)</visible>
		</control>
		<control type="label" id="2"><!-- Dialog heading -->
			<left>22</left>
			<top>6</top>
			<width>882</width>
			<height>46</height>
			<align>center</align>
			<font>font18</font>
		</control>
		<control type="image">
			<left>22</left>
			<top>54</top>
			<width>882</width>
			<height>4</height>
			<texture>separator.png</texture>
		</control>
		<control type="button" id="999"><!-- Touch Close button -->
			<left>902</left>
			<top>-10</top>
			<width>32</width>
			<height>32</height>
			<texturefocus>touch-close.png</texturefocus>
			<texturenofocus>touch-close.png</texturenofocus>
			<onclick>Dialog.Close(osdaudiodspsettings)</onclick>
			<visible>System.GetBool(input.enablemouse)</visible>
		</control>
		<control type="group">
			<control type="grouplist" id="5"><!-- control area -->
				<left>22</left>
				<top>60</top>
				<width>882</width>
				<height>560</height>
				<itemgap>-2</itemgap>
				<pagecontrol>30</pagecontrol>
				<onback>Dialog.Close(osdaudiodspsettings)</onback>
				<onleft condition="StringCompare(Skin.String(show-scrollbar),$LOCALIZE[31877])">noop</onleft>
				<onleft condition="!StringCompare(Skin.String(show-scrollbar),$LOCALIZE[31877])">30</onleft>
				<onright>Dialog.Close(osdaudiodspsettings)</onright>
				<onup>5</onup>
				<ondown>5</ondown>
			</control>
			<control type="scrollbar" id="30">
				<left>10</left>
				<top>112</top>
				<width>18</width>
				<height>545</height>
				<onright>2</onright>
				<animation condition="!Control.HasFocus(30) + !Container.Scrolling + !StringCompare(Skin.String(show-scrollbar),$LOCALIZE[31875])" effect="fade" start="100" end="0" time="200" reversible="true">Conditional</animation>
				<visible>!StringCompare(Skin.String(show-scrollbar),$LOCALIZE[31877])</visible>
			</control>
		</control>
		<control type="button" id="7"><!-- Default Button -->
			<height>56</height>
			<font>font12</font>
		</control>
		<control type="radiobutton" id="8"><!-- Default RadioButton -->
			<height>56</height>
			<font>font12</font>
		</control>
		<control type="spincontrolex" id="9"><!-- Default spincontrolex -->
			<height>56</height>
			<font>font12</font>
			<reverse>yes</reverse>
		</control>
		<control type="image" id="11"><!-- Default Seperator -->
			<height>4</height>
			<texture>separator.png</texture>
		</control>
		<control type="edit" id="12"><!-- Default Edit -->
			<height>56</height>
			<font>font12</font>
		</control>
		<control type="sliderex" id="13"><!-- Default Slider -->
			<height>56</height>
			<font>font12</font>
			<animation effect="fade" start="50" end="50" time="0" condition="Player.Passthrough">Conditional</animation>
		</control>
		<control type="label" id="14"><!-- Default Label -->
			<height>56</height>
			<font>font12</font>
			<align>center</align>
		</control>
	</controls>
</window>
