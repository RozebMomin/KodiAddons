<?xml version="1.0" encoding="UTF-8"?>
<snappers xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="https://code.google.com/p/apple-tv2-xbmc/snappers/ snappers.xsd"
